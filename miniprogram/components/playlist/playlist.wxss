/* components/playlist/playlist.wxss */
/* gan! */
.playlist-container{
  width: 220rpx;
  position: relative;
  padding-bottom: 20rpx;
}

.playlist-img{
  width: 100%;
  height: 220rpx;
  border-radius: 10rpx;
}

.playlist-playcount{
  font-size:24rpx;
  color:#fff;
  text-shadow: 1px 0 0 rgba(0, 0, 0,0.15);
  position: absolute;
  right: 10rpx;
  top: 4rpx;
  padding-left: 26rpx;
  background: url(data:image/png;base64,) no-repeat 0.8rpx/22rpx 20rpx;
}

.playlist-name{
  font-size: 26rpx;
  line-height: 1.2;
  padding: 4rpx 0 0 12rpx;
  display: -webkit-box;
  -wekbit-box-orient:vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
}